@article{schlecht:2008:genomewide,
  pages={2193-207},
  title={Genome-wide expression profiling, in vivo {DNA} binding analysis, and probabilistic motif prediction reveal novel Abf1 target genes during fermentation, respiration, and sporulation in yeast.},
