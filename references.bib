@article{baranello:2016:rna,
  pages={357-71},
  title={{RNA} Polymerase {II} Regulates Topoisomerase 1 Activity to Favor Efficient Transcription.},
  doi={10.1016/j.cell.2016.02.036},
  author={Baranello, Laura and Wojtowicz, Damian and Cui, Kairong and Devaiah, Ballachanda N and Chung, {Hye-Jung} J and {Chan-Salis,} Ka Y and Guha, Rajarshi and Wilson, Kelli and Zhang, Xiaohu and Zhang, Hongliang and Piotrowski, Jason and Thomas, Craig J and Singer, Dinah S and Pugh, {BF} and Pommier, Yves and Przytycka, Teresa M and Kouzine, Fedor and Lewis, Brian A and Zhao, Keji and Levens, David},
