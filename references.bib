  author={de Bruin, Alain and A Cornelissen, Peter W and Kirchmaier, Bettina C and Mokry, Michal and Iich, Elhadi and Nirmala, Ella and Liang, {Kuo-Hsuan} H and D V{\'e}gh, Anna M and Scholman, Koen T and Groot Koerkamp, Marian J and Holstege, Frank C and Cuppen, Edwin and {Schulte-Merker,} Stefan and Bakker, Walbert J},
  volume={44},
