  author={Hobor, Fruzsina and Pergoli, Roberto and Kubicek, Karel and Hrossova, Dominika and Bacikova, Veronika and Zimmermann, Michal and Pasulka, Josef and Hofr, Ctirad and Vanacova, Stepanka and Stefl, Richard},
