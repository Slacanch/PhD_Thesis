@article{oconnell:1995:role,
  pages={1879-88},
  title={Role of the Saccharomyces cerevisiae general regulatory factor {CP1} in methionine biosynthetic gene transcription.},
  author={{O'Connell,} {KF} and {Surdin-Kerjan,} Y and Baker, {RE}},
  volume={15},
