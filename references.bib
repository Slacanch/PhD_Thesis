@article{hu:1999:chromatin,
  pages={637-42},
  title={Chromatin remodeling and activation of chromosomal {DNA} replication by an acidic transcriptional activation domain from {BRCA1.}},
  author={Hu, {YF} and Hao, {ZL} and Li, R},
  volume={13},
  number={6},
