@article{cheng:2011:rna,
  pages={395-410},
  title={{RNA} splicing and debranching viewed through analysis of {RNA} lariats.},
  doi={10.1007/s00438-011-0635-y},
  author={Cheng, Zhi and Menees, Thomas M},
