@article{fishburn:2016:transcription,
  pages={13040-7},
  title={Transcription Start Site Scanning and the Requirement for {ATP} during Transcription Initiation by {RNA} Polymerase {II.}},
  doi={10.1074/jbc.M116.724583},
  author={Fishburn, James and Galburt, Eric and Hahn, Steven},
