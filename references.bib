@article{holstege:1996:opening,
  pages={1666-77},
  title={Opening of an {RNA} polymerase {II} promoter occurs in two distinct steps and requires the basal transcription factors {IIE} and {IIH.}},
  author={Holstege, {FC} and van der Vliet, {PC} and Timmers, {HT}},
  volume={15},
