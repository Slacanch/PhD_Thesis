@article{martintumasz:2015:saccharomyces,
  pages={22880-9},
  title={Saccharomyces cerevisiae Sen1 Helicase Domain Exhibits 5'- to {3'-Helicase} Activity with a Preference for Translocation on {DNA} Rather than {RNA.}},
